{
  "manifest_version": 2,

  "name": "Final Fantasy",
  "description": "The last ESPN fantasy UI you'll need",
  "version": "0.0.1",
  "icons": {
  },
  "content_scripts": [
    {
      "matches": ["http://games.espn.go.com/*", "https://games.espn.go.com/*"],
      "js": ["build/main.js"]
    }
  ],
  "web_accessible_resources": [
    "build/*"
  ],
  "permissions": [
    "activeTab"
  ]
}
